{
  "name": "primo",
  "version": "1.0.0",
  "description": "Pre-interview coding challenge",
  "main": "dist/app.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "prebuild": "npm run clean",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Captain Phungpensuk",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "eslint": "^9.18.0",
    "husky": "^9.1.0",
    "jest": "^29.7.0",
    "jiti": "^2.4.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  }
}
